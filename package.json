{
  "name": "on-board-poc",
  "version": "0.0.0",
  "scripts": {
    "start-dev": "npm-run-all -l -p start-frontnd watch-backnd",
    "ng": "ng",
    "start-frontnd": "ng serve --host 0.0.0.0 --proxy-config proxy.config.json",
    "build-frontnd": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-backnd": "npm run prod",
    "server": "babel-node -r node_modules/dotenv/config ./server",
    "server:prod": "node ./dist",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build-backnd": "babel ./server --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build-backnd server:prod",
    "watch-backnd": "nodemon"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.12",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "find-config": "^1.0.0",
    "logrocket": "^1.0.3",
    "neo4j-driver": "^1.7.6",
    "npm-run-all": "^4.1.5",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1",
    "rimraf": "^3.0.0",
    "rxjs": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^1.19.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
